{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(lldb) Launch zimdjson",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/main",
      "args": ["tests/large-file.json"],
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build-zimdjson"
    },
    {
      "name": "(lldb) Launch simdjson",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/simdjson/main",
      "args": ["../tests/large-file.json"],
      "cwd": "${workspaceFolder}/simdjson",
      "preLaunchTask": "build-simdjson"
    }
  ]
}
